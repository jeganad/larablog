{
    "name": "jeganad/larablog",
    "type": "package",
    "description": "A minimal blogging package.",
    "keywords": [
        "larablog",
        "laravel",
        "blog"
    ],
    "homepage": "https://github.com/jeganad/larablog",
    "license": "MIT",
    "authors": [
        {
            "name": "Jegan",
            "email": "jeganad@yahoo.com",
            "homepage": "hhttps://github.com/jeganad"
        }
    ],
    "require": { 
        "php": "^7.3",
        "illuminate/support": "*",
        "naoray/larablog-default-theme": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.7",
        "orchestra/testbench": "~3.4",
        "illuminated/testing-tools": "^0.5.5"
    },
    "autoload-dev": {
        "psr-4": {
            "Naoray\\Larablog\\Test\\": "tests"
        }
    },
    "autoload": {
        "psr-4": {
            "Naoray\\Larablog\\": "app/"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "minimum-stability": "stable"
}
